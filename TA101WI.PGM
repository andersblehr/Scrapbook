/******************************************************************************
*
*    Callback       :    Ta101cbOtherGUIEvent
*
*    Control/Event  :    WINDOW / OTHER_GUI_EVENT
*
*    Description    :
*
*
*
*
*    Revision History:
*    Date      Revised By   Description
*    --------  -----------  -------------------------------------------------
*    03/30/95  AB           Created.
*
*******************************************************************************/
static WCBPROC (Ta101cbOtherGUIEvent)
{
    PSWP                pSWP;
    SHORT               i;
    TTA_INT7_DATETIME   stDateTime;
    CHAR                szAMPM[3];
    STATIC BOOL         fMenuEnd = FALSE;

    TaErrCbkStart ("Ta101cbOtherGUIEvent");

    switch (CBI_IWApplMsg)
    {
        case WM_ACTIVATE :

            if (((BOOL)CBI_GUImp1) && (!TaWimQueryActiveHWND()))
                TaMsgInterWinPost( CBI_hwnd,
                                   (PTTA_VOID)&CBI_hwnd,
                                   CTA_MSG_WIN_RECEIVE_FOCUS );

            break;

        case WM_TIMER :

            /* Check if timer is the desktop clock timer */
            if ((USHORT)CBI_GUImp1 == CTA_TID_TASCLOCK)
            {
                /* Increase seconds counter */
                usSecs++;

                /* Update if 60 seconds since last update */
                if (usSecs == 60)
                {
                    /* Get the current date and time */
                    TaOpsGetDateTime(&stDateTime);

                    /* Format and print the time on the desktop */
                    if (stDateTime.hour < 12)
                    {
                        if (stDateTime.hour == 0)
                            stDateTime.hour = 12;

                        strcpy(szAMPM, "AM");
                    }
                    else
                    {
                        if (stDateTime.hour > 12)
                            stDateTime.hour -= 12;

                        strcpy(szAMPM, "PM");
                    }

                    sprintf( TxGneric1Data, "%2.2u:%2.2u %s",
                             stDateTime.hour,
                             stDateTime.min,
                             szAMPM                           );

                    /* Reset seconds counter */
                    usSecs = 0;

                    /* Print new date if we're past midnight */
                    if (stDateTime.day != iDate)
                    {
                        sprintf( TxGnericData, "%2.2u/%2.2u/%2.2u",
                                 stDateTime.month,
                                 stDateTime.day,
                                 stDateTime.year % 100              );

                        iDate = stDateTime.day;
                    }
                }
            }

            break;

        case WM_MENUEND :

            /* Indicate that we're leaving a menu */
            fMenuEnd = TRUE;

            break;

        case WM_MENUSELECT :

            /* Set focus to active window if control returns to desktop */
            if ((fMenuEnd                    ) &&
                ((ULONG)CBI_GUImp1 == 0xffff ) &&
                ((HWND)CBI_GUImp2 == hMenuBar)    )
            {
                /* Only set focus if a window is indeed active */
                if (TaWimQueryActiveHWND())
                    TaWimSetFocus(TaWimQueryActiveHWND());

                /* We've left the menu */
                fMenuEnd = FALSE;
            }

            break;

        case WM_MINMAXFRAME :

            /* Get the Set Window Position structure from MP1 */
            pSWP = (PSWP)CBI_GUImp1;

            if (pSWP->fl & SWP_MINIMIZE)
            {
                /* Hide all non-minimized child windows */
                for (i = 0; i < pstWindowList->usNumWindows; i++)
                    if (!pstWindowList->apstWindow[i]->fMinimized)
                        FndWindowHide( pstWindowList->apstWindow[i]->hHWND,
                                       (PTTA_FND_ERROR_BLOCK)pABHI          );
            }
            else if (pSWP->fl & SWP_RESTORE)
            {
                /* Show all non-minimized child windows */
                for (i = 0; i < pstWindowList->usNumWindows; i++)
                    if (!pstWindowList->apstWindow[i]->fMinimized)
                        FndWindowShow( pstWindowList->apstWindow[i]->hHWND,
                                       (PTTA_FND_ERROR_BLOCK)pABHI          );

                /* Set focus to the active window */
                TaWimSetFocus(TaWimQueryActiveHWND());
            }

            break;
    }

    TaErrCbkStop ("Ta101cbOtherGUIEvent");
    return (0);

} /* end Ta101cbOtherGUIEvent */


