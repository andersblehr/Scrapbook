*    Callback       :    Ta101cbMessageNotification
*
*    Control/Event  :    WINDOW / MSG_NOTIFICATION
*
*    Description    :
*
*
*    Revision History:
*    Date      Revised By   Description
*    --------  -----------  -------------------------------------------------
*    03/29/95  AB           Created.
*
*******************************************************************************/
static WCBPROC (Ta101cbMessageNotification)
{
    TaErrCbkStart ("Ta101cbMessageNotification");

    TaMsgRoute();

    if( ( TaErrMsgNum == ETA_OK ) &&
        ( ((PTTA_ARCH_HDR_RECV)CBI_pMSGData)->szLBoxName[0] != 0 ))
    
        TaLstSetCursorPos( ((PTTA_ARCH_HDR_RECV)CBI_pMSGData)->szLBoxName,
                           ((PTTA_ARCH_HDR_RECV)CBI_pMSGData)->WIN_hwnd,
                           CBI_pMSGData );

    TaErrCbkStop ("Ta101cbMessageNotification");
    return (0);

} /* end Ta101cbMessageNotification */


/******************************************************************************
*
*    Callback       :    Ta101cbInterwindowMessage
*
*    Control/Event  :    WINDOW / INTERWINDOW_MSG
*
*    Description    :
*
*
*
*
*    Revision History:
*    Date      Revised By   Description
*    --------  -----------  -------------------------------------------------
*    03/29/95  AB           Created.
*
*******************************************************************************/
static WCBPROC (Ta101cbInterwindowMessage)
{
    USHORT          usIndex;
    TTA_WINHEADER   stHeaderKeep;
    SHORT           i;

    TaErrCbkStart ("Ta101cbInterwindowMessage");

    switch (CBI_IWApplMsg)
    {
        case CTA_MSG_WIN_RECEIVE_FOCUS :

            if (TaWimValidHandle(*((HWND *)CBI_IWApplParm), &usIndex))
            {
                if (*((HWND *)CBI_IWApplParm) != TaWimQueryActiveHWND())
                {
                    /* Keep the window header of the next active window */
                    stHeaderKeep = *(pstWindowList->apstWindow[usIndex]);

                    /* Move the next active window to the fron of the list */
                    for (i = usIndex; i > 0; i--)
                        *(pstWindowList->apstWindow[i]) =
                                *(pstWindowList->apstWindow[i - 1]);

                    *(pstWindowList->apstWindow[0]) = stHeaderKeep;
                }

                /* Activate the window's dynamic menu items */
                _TaMenActivate(&(pstWindowList->apstWindow[0]->stMenuData));

                /* Check the window's item in the Windows menu */
                TaMenWindowItemCheck(
                        pstWindowList->apstWindow[0]->hHWND,
                        pstWindowList->apstWindow[1]
                            ? pstWindowList->apstWindow[1]->hHWND
                            : 0                                    );

                /* Set the window's desktop title */
                _TaDesSetTitle( TaWimQueryActiveHWND(),
                                NULL,
                                pABHI,
                                __FILE__,
                                __LINE__  );
            }
            else
                /* Window list is empty */
                _TaDesSetTitle( CBI_hwnd,
                                NULL,
                                pABHI,
                                __FILE__,
                                __LINE__  );

            break;

        case CTA_MSG_WIN_LOSE_FOCUS :

            break;
    }

    TaErrCbkStop ("Ta101cbInterwindowMessage");
    return (0);

} /* end Ta101cbInterwindowMessage */


/******************************************************************************
*
*    Callback       :    Ta101cbPredisplay
*
*    Control/Event  :    WINDOW / PREDISPLAY
*
*    Description    :
*
*
*
*
*    Revision History:
*    Date      Revised By   Description
*    --------  -----------  -------------------------------------------------
*    03/29/95  AB           Created.
*
*******************************************************************************/
static WCBPROC (Ta101cbPredisplay)
{
    TTA_INT7_DATETIME   stDateTime;
    _FND_PROG_ARGUMENTS *pProgArguments;
    CHAR                szAMPM[3];

    TaErrCbkStart ("Ta101cbPredisplay");

    /* Move the desktop to its default position */
    FndWindowMove( CBI_hwnd,
                   MAIN_WINDOW_X_POS,
                   MAIN_WINDOW_Y_POS,
                   (PTTA_FND_ERROR_BLOCK)pABHI );

    /* Get the current date and time */
    TaOpsGetDateTime(&stDateTime);

    /* Print the date on the desktop */
    sprintf( TxGnericData, "%2.2u/%2.2u/%2.2u",
             stDateTime.month,
             stDateTime.day,
             stDateTime.year % 100              );

    /* Format and print the time on the desktop */
    if (stDateTime.hour < 12)
    {
        if (stDateTime.hour == 0)
            stDateTime.hour = 12;

        strcpy(szAMPM, "AM");
    }
    else
    {
        if (stDateTime.hour > 12)
            stDateTime.hour -= 12;

        strcpy(szAMPM, "PM");
    }

    sprintf( TxGneric1Data, "%2.2u:%2.2u %s",
             stDateTime.hour,
             stDateTime.min,
             szAMPM                           );

    /* Store date and initialize seconds counter */
    iDate = stDateTime.day;
    usSecs = stDateTime.sec;

    /* Get the handle of the menu bar */
    hMenuBar = WinWindowFromID(CBI_hwnd, FID_MENU);

    /* Get the application anchor block */
    hab = WinQueryAnchorBlock(CBI_hwnd);

    /* Start the desktop clock timer */
    WinStartTimer(hab, CBI_hwnd, CTA_TID_TASCLOCK, CTA_TASCLOCK_FREQ);

    /* Initialize the service table */
    TaMsgInitSvcTbl();

    /* Allocate, initialize and retrieve the window list */
    pstWindowList = TaWimCreateWindowList();

    /* Supply the TaMen class with the handle of the desktop */
    _TaMenMainPredisplay( CBI_hwnd );

    /* Inform the TaMen class that no windows are active */
    _TaMenActivate(NULL);

    /* Enable launching of testwindows facility: Get cmd line argument(s) */
    pProgArguments = (_FND_PROG_ARGUMENTS *)CBI_pInitData;

    /* If a test DLL has been passed, check that it's a valid DLL */
    if (pProgArguments->FndNumArguments >= 1)
        pszTestWindowName = strtok(pProgArguments->FndArgumentStr, "\t");
    else
        /* Hide the launch button if no test DLL was passed */
        TaWidCmdHide("TA014PB_LAUNCH_TEST_WINDOW");

    /* Hide Windows menu menu items (no windows are open yet) */
    FndCommandHide(CBI_hwnd, "TA074MI_WIN_WINDOW_1", (PTTA_FND_ERROR_BLOCK)pABHI);
    FndCommandHide(CBI_hwnd, "TA075MI_WIN_WINDOW_2", (PTTA_FND_ERROR_BLOCK)pABHI);
    FndCommandHide(CBI_hwnd, "TA076MI_WIN_WINDOW_3", (PTTA_FND_ERROR_BLOCK)pABHI);
    FndCommandHide(CBI_hwnd, "TA077MI_WIN_WINDOW_4", (PTTA_FND_ERROR_BLOCK)pABHI);
    FndCommandHide(CBI_hwnd, "TA078MI_WIN_WINDOW_5", (PTTA_FND_ERROR_BLOCK)pABHI);

    /* Hide taxpayer items on File menu */
    FndCommandHide(CBI_hwnd, "TA003MI_FIL_TAXPAYER_1", (PTTA_FND_ERROR_BLOCK)pABHI);
    FndCommandHide(CBI_hwnd, "TA004MI_FIL_TAXPAYER_2", (PTTA_FND_ERROR_BLOCK)pABHI);
    FndCommandHide(CBI_hwnd, "TA005MI_FIL_TAXPAYER_3", (PTTA_FND_ERROR_BLOCK)pABHI);
    FndCommandHide(CBI_hwnd, "TA006MI_FIL_TAXPAYER_4", (PTTA_FND_ERROR_BLOCK)pABHI);
    FndCommandHide(CBI_hwnd, "TA007MI_FIL_TAXPAYER_5", (PTTA_FND_ERROR_BLOCK)pABHI);

    /* Read .INI file */
    TaDesReadIniFile();

    TaErrCbkStop ("Ta101cbPredisplay");
    return (0);

} /* end Ta101cbPredisplay */


