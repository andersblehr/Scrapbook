static WCBPROC (Ta101cbFilPrint)
{
    SWP     stWindowPos;

    TaErrCbkStart ("Ta101cbFilPrint");

    if (WinQueryWindowPos(TaWimQueryActiveHWND(), &stWindowPos)      &&
        WinSetPointerPos(HWND_DESKTOP, stWindowPos.x, stWindowPos.y)    )
        WinPostMsg( TaWimQueryActiveHWND(),
                    WM_CHAR,
                    CTA_PRINTSCREEN_MP1,
                    CTA_PRINTSCREEN_MP2     );

    TaErrCbkStop ("Ta101cbFilPrint");
    return (0);

} /* end Ta101cbFilPrint */


