/******************************************************************************
*
*    Function       :    vDisplayMessageOK
*
*    Description    :    Displays the message in an information box with
*                        an OK button.  Used to indicate what action will
*                        be performed when a yet unimplemented event occurs.
*
*
*    Revision History:
*    Date      Revised By   Description
*    --------  -----------  -------------------------------------------------
*    04/21/95  AB           Created.
*
*******************************************************************************/
static VOID vDisplayMessageOK ( PTTA_CHAR           pszMessage,
                                PCALLBACKEVENTINFO  pEventInfo  )
{
    USHORT              usSelButton;
    _FND_ERROR_BLOCK    stErrorBlock;

    FndMsgBoxDisplayText( pszMessage,
                          NULL,
                          CBI_hwnd,
                          FND_MSGBOX_OK,
                          FND_MSGBOX_INFORMATION,
                          FND_MSGBOX_IDOK,
                          NULL,
                          &usSelButton,
                          &stErrorBlock           );

} /* end vDisplayMessageOK */


